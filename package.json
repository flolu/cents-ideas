  "scripts": {
    "setup": "yarn install && yarn env:setup && yarn envsub:dev",
    "dev": "yarn envsub:dev && yarn docker",
    "client:dev": "yarn envsub:dev && ibazel run //services/client:dev_server",
    "admin:dev": "yarn envsub:dev && ibazel run //services/admin-client:live_server",
    "client": "yarn envsub:dev && bazelisk run //services/client",
    "admin": "yarn envsub:dev && bazelisk run //services/admin-client",
    "test": "yarn envsub:dev && bazelisk test //...",
    "deploy": "yarn envsub:prod && bazelisk run :k8s.apply",
    "clean": "yarn env:clean && rm -rf node_modules && bazelisk clean && yarn docker:clean",
    "lint": "tslint --project .",
    "up": "yarn upgrade-interactive --latest",
    "envsub:dev": "yarn env:setup && concurrently \"yarn:envsub:*:dev\" \"yarn:envsub:k8s:*\"",
    "envsub:prod": "yarn env:setup && concurrently \"yarn:envsub:*:prod\" \"yarn:envsub:k8s:*\"",
    "envsub:k8s:config": "envsub --env-file .env.prod $npm_package_config_k8s_template $npm_package_config_k8s",
    "envsub:k8s:secret": "envsub --env-file .env.prod $npm_package_config_k8s_secret_template $npm_package_config_k8s_secret",
    "envsub:client:prod": "envsub --env-file .env.prod $npm_package_config_client_env_template $npm_package_config_client_env",
    "envsub:client:dev": "envsub --env-file .env.dev $npm_package_config_client_env_template $npm_package_config_client_env",
    "envsub:admin:prod": "envsub --env-file .env.prod $npm_package_config_admin_env_template $npm_package_config_admin_env",
    "envsub:admin:dev": "envsub --env-file .env.dev $npm_package_config_admin_env_template $npm_package_config_admin_env",
    "env:clean": "rm -f $npm_package_config_client_env && rm -f $npm_package_config_admin_env && rm -f $npm_package_config_k8s && rm -f $npm_package_config_k8s_secret",
    "env:setup": "ts-node misc/setup",
    "docker": "docker-compose up --build --no-color 2>&1 | sed 's/^[^ ]*  *| //'",
    "docker:clean": "yarn docker:stop && docker system prune --volumes",
    "docker:stop": "docker stop $(docker ps -q)",
    "vapid": "web-push generate-vapid-keys --json",
    "postinstall": "ngcc"
  },
  "config": {
    "client_env_template": "services/client/environment/environment.template.ts",
    "client_env": "services/client/environment/environment.ts",
    "admin_env_template": "services/admin-client/environment/environment.template.ts",
    "admin_env": "services/admin-client/environment/environment.ts",
    "k8s_template": "packages/kubernetes/config.template.yaml",
    "k8s": "packages/kubernetes/config.yaml",
    "k8s_secret_template": "packages/kubernetes/secret.template.yaml",
    "k8s_secret": "packages/kubernetes/secret.yaml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write --parser=typescript",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  }
}
