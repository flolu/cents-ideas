{
  "name": "@cents-ideas/frontend",
  "version": "0.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "yarn envsub && node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run frontend:server:production --bundleDependencies all",
    "envsub": "envsub ./dist/browser/assets/settings.template.json ./dist/browser/assets/settings.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/platform-server": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@angular/service-worker": "~9.0.1",
    "@briebug/jest-schematic": "^1.7.0",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "async-retry": "^1.3.1",
    "express": "^4.15.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@ngrx/effects": "^8.5.1",
    "@ngrx/entity": "^8.5.1",
    "@ngrx/store": "^8.5.1",
    "@ngrx/store-devtools": "^8.5.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "envsub": "^3.0.9",
    "jest": "24.9.0",
    "jest-preset-angular": "7.1.1",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5",
    "webpack-cli": "^3.1.0"
  }
}
