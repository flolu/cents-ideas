{
  "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/**/*.css",
          "/**/*.js",
          "!/system.js",
          "!/core.min.js",
          "!/bundle-*.min/*.js",
          "/bundle-es5.min/index.js",
          "/bundle-es2015.min/index.js",
          "!/node_modules/**/*.js"
        ],
        "urls": ["https://fonts.googleapis.com/**", "https://fonts.gstatic.com/s/**"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": ["http://localhost:3000/**", "https://api.cents-ideas.flolu.com/**"],
      "cacheConfig": {
        "maxAge": "1d",
        "maxSize": 100,
        "strategy": "freshness",
        "timeout": "10s"
      }
    }
  ]
}
