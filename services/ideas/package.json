{
  "name": "@cents-ideas/ideas",
  "version": "0.4.0",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon --ext ts --watch src --exec \"yarn start\" --ignore src/**/*.spec.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist && rimraf coverage && rimraf node_modules"
  },
  "dependencies": {
    "@cents-ideas/enums": "^0.4.0",
    "@cents-ideas/event-sourcing": "^0.4.0",
    "@cents-ideas/models": "^0.4.0",
    "@cents-ideas/utils": "^0.4.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/shortid": "^0.0.29",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testMatch": [
      "<rootDir>/**/*.spec.ts"
    ]
  }
}
